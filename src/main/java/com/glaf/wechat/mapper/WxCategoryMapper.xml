	<delete id="deleteWxCategories" parameterType="com.glaf.wechat.query.WxCategoryQuery">
		delete from WX_CATEGORY
		where (
		ID_ IN
		<foreach item="x_id" index="index" collection="ids" open="("
			separator="," close=")">
			#{x_id}
		</foreach>
		)
	</delete>


