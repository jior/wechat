		<result property="multiFlag" column="MULTIFLAG_" jdbcType="INTEGER" />
		<result property="limitFlag" column="LIMITFLAG_" jdbcType="INTEGER" />
		<result property="limitTimeInterval" column="LIMITTIMEINTERVAL_"
